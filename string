package day7;
public class string{
	public static void main(String[] args) {
	   String str="apple";
	   String str1="apple";
	   String str2=new String("pineapple");
	   System.out.println(str==str2);
	   System.out.println(str.equals(str2));
	   System.out.println(str2.length());
	   System.out.println(str.trim());
	   System.out.println(str.equalsIgnoreCase(str));
	   System.out.println(str.trim().length());
	   System.out.println(str.toUpperCase());
	   System.out.println(str.toLowerCase());
	   System.err.println(str2.substring(1,6));
	   String word[]=str.split(str2);
	   for(String var:word) {
	   System.out.println(var);
	   }
	   System.out.println(str2.contains("fruit"));
	   System.out.println(str2.startsWith("pi"));
	   System.out.println(str2.endsWith(str));
	   System.out.println(str2.charAt(4));
	   System.out.println(str2.concat(" fruit"));
	}
   
	}
