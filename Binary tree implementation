import java.util.*;
class node{
  int data;
  node L,R;
  node(int d)
  {
    data=d;
    L=R=null;
  }
}
public class Main{
  static node root=null;
  public static void main(String[]args){
    root=new node(12);
  root.L=new node(1);
  root.R=new node(6);
  root.L.L=new node(7);
  root.L.R=new node(13);
  root.R.L=new node(10);
System.out.println(root.data);
System.out.println(root.L.L.data);
}
}
