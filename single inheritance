package day9;

	abstract class Bike
	{
		abstract void run();
	}
	class Honda extends Bike
	{
		void run()
		{
			System.out.println("Vehicle");
		}
		void play()
		{
			System.out.println("playing");
		}
	}
	public class problem1{
	public static void main(String[]args)
	{
		Bike obj=new Honda();
		obj.run();
	}
	}

