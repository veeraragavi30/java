import java.util.Scanner;

public class QuantumMeanCalculator {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        double sum = 0.0;

        for (int i = 0; i < n; i++) {
            sum += sc.nextDouble();
        }
        double mean = sum / n;
        String formattedMean = String.format("%.2f", mean);

        System.out.println("Quantum Mean Calculation Completed:");
        System.out.println("The quantum mean of the array is " + formattedMean);
        System.out.println("Congratulations, Quantum Explorer!");
    }
}
