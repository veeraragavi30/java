public class problem3 {

	public static void main(String[] args) {
		int num =5;
		int num2=6;
		int num3=7;
		int num4=8;
		int num5=3;
		num+=3;
		num2-=3;
		num3*=3;
		num4/=5;
		num5%=3;
		System.out.println(num);
		System.out.println(num2);
        System.out.println(num3);
        System.out.println(num4);
        System.out.println(num5);
	}

}
