import java.util.Scanner;
public class Main{
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int x = scanner.nextInt();
        int y = scanner.nextInt();
        double ti = scanner.nextDouble();
        double tj = scanner.nextDouble(); 
        if (x <= 0 || x >= 32767 || y <= 0 || y >= 32767) {
            System.out.println("Invalid Input");
            return;
        }

            if (tj >= ti) {
            System.out.println("Invalid Input");
            return;
        }
        int tiHours = (int) ti;
        int tiMinutes = (int) ((ti - tiHours) * 100);
        int totalMinutesTi = tiHours * 60 + tiMinutes;

        int tjHours = (int) tj;
        int tjMinutes = (int) ((tj - tjHours) * 100);
        int totalMinutesTj = tjHours * 60 + tjMinutes;

        double timeAvailable = (totalMinutesTi - totalMinutesTj) / 60.0;
        double timeRequired = (double) x / y;

        if (timeRequired <= timeAvailable) {
            System.out.println("Yes");
        } else {
            System.out.println("No");
        }
    }
}
